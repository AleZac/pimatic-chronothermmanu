# This is an example on how to load the plugin in the config.json. It is NOT the complete config file of Pimatic!

"plugins": [
  {
    "plugin": "ChronoThermManuDevice"
  }
],

"devices": [
  {
    "id": "provacas",
    "class": "ChronoThermManuDevice",
    "name": "Chrono Therm Manual",
    "boost": true,
    "realtemperature": "$realtemperature",
    "interval": 240,
    "offtemperature": 4,
    "ontemperature": 21
    "valve": "$rele"
  }
]
